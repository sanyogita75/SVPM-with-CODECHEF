#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int n,x,y;
	int t;
	cin>>t;
	
	while(t--){
	    cin>>n>>x>>y;
	    int a[n];
	    for(int i=0;i<n;i++){
	        cin>>a[i];
	    }
	    int c=0;
	    for(int i=0;i<n;i++){
	        if(a[i]<=x and a[i]%y==0){
	            c++;
	        }
	        
	    }
	    cout<<c<<endl;
	}
	return 0;
}
